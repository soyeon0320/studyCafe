package kr.co.scm.store.vo;

import kr.co.scm.base.vo.BaseVO;
import kr.co.scm.order.vo.ProductVO;

public class EquipVO extends ProductVO{
	private String equipCode;
	private String equipName;
	private String equipManufacture;
	private String storeCode;
	private int equipUseable;
	private int equipRentPrice;
	private int equipCnt;
	
	
	public String getEquipCode() {
		return equipCode;
	}
	public void setEquipCode(String equipCode) {
		this.equipCode = equipCode;
	}
	public String getEquipName() {
		return equipName;
	}
	public void setEquipName(String equipName) {
		this.equipName = equipName;
	}
	public String getEquipManufacture() {
		return equipManufacture;
	}
	public void setEquipManufacture(String equipManufacture) {
		this.equipManufacture = equipManufacture;
	}
	public String getStoreCode() {
		return storeCode;
	}
	public void setStoreCode(String storeCode) {
		this.storeCode = storeCode;
	}
	public int getEquipUseable() {
		return equipUseable;
	}
	public void setEquipUseable(int equipUseable) {
		this.equipUseable = equipUseable;
	}
	public int getEquipRentPrice() {
		return equipRentPrice;
	}
	public void setEquipRentPrice(int equipRentPrice) {
		this.equipRentPrice = equipRentPrice;
	}
	public int getEquipCnt() {
		return equipCnt;
	}
	public void setEquipCnt(int equipCnt) {
		this.equipCnt = equipCnt;
	}
	
}
